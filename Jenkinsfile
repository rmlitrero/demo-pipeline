pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building jenkins pipeline for Webhooks'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing jenkins pipeline'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying deployment of kubernetes pipeline as CD'
            }
        }
    }
}
